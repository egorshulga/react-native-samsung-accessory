<resources>
  <application name="ReactNativeAccessory">
    <serviceProfile
      role="provider"
      name="react-native-accessory"
      id="/react-native/accessory"
      serviceImpl="com.egorshulga.reactnative.samsungaccessory.AccessoryProvider"
      version="1.0">

      <supportedTransports>
        <transport type="TRANSPORT_BT" />
        <transport type="TRANSPORT_BLE" />
        <transport type="TRANSPORT_WIFI" />
        <transport type="TRANSPORT_USB" />
      </supportedTransports>

      <serviceChannel
        dataRate="high"
        id="901"
        priority="medium"
        reliability="enable" />
    </serviceProfile>
  </application>
</resources>
